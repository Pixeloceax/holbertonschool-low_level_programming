#include "main.h"

/**
 * _isalpha - lower case and upper case send 1
 *
 * @c: variable
 *
 * Return: 1 is lower and upper case 0 to the other
 */

int print_sign(int n)
{
	if (n > 0)
	{
		return (1);
		_putchar (43);
	}
	if else (n = 0)
	{
		return (0);
		_putchar (48);
	}
	else
	  {
	    return (-1);
	    _putchar(45);
	    
	  }
	
}
