#include <stdlib.h>
#include <time.h>
/* more headers goes there */

/* betty style doc for function main goes there */
int main(void)
{
int n;

srand(time(0));
n = rand() - RAND_MAX / 2;
if (0 < n)
{
printf("is positive\n");
}
else if(0 == n)
{
printf("is zero\n");
}
else(0 > n)
{       
printf("is negative\n");
}
return (0);
}
